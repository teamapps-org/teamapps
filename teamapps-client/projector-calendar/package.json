{
  "name": "projector-calendar",
  "version": "0.9.155-SNAPSHOT",
  "type": "module",
  "exports": "./target/js-dist/index.js",
  "types": "./target/js-dist/lib/index.d.ts",
  "scripts": {
    "del_dist": "rimraf 'dist/*'",
    "clean": "yarn run del_dist",
    "tsc": "tsc",
    "bundle": "node esbuild.mjs",
    "build": "yarn run del_dist && yarn run tsc && yarn run bundle",
    "build-prod": "NODE_ENV=production yarn run build",
    "start": "yarn run watch-build",
    "watch-build": "concurrently --names \"yarn:watch-tsc\" \"yarn:watch-bundle\"",
    "watch-bundle": "yarn run bundle -- --watch",
    "watch-tsc": "yarn run tsc --watch"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-var": "^1.1.0",
    "esbuild": "^0.15.13",
    "onchange": "^7.1.0",
    "replace-in-file": "^6.3.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@fullcalendar/core": "4.4.2",
    "@fullcalendar/daygrid": "4.4.2",
    "@fullcalendar/interaction": "4.4.2",
    "@fullcalendar/moment": "4.4.2",
    "@fullcalendar/moment-timezone": "4.4.2",
    "@fullcalendar/timegrid": "4.4.2",
    "d3": "^5.9.7",
    "d3-luxon": "^1.0.2",
    "moment-timezone": "^0.5.43",
    "teamapps-client-core": "workspace:*"
  }
}
